<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal">
<!-- 		<RelativeLayout android:layout_width="fill_parent" android:id="@+id/rellayout" -->
<!-- 			android:layout_height="wrap_content" android:orientation="horizontal"> -->

			<TextView android:id="@+id/guid" android:text="guid"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:textSize="13dp" />
				
			<TextView android:id="@+id/id" android:text="id#"
				android:layout_width="wrap_content"
				android:layout_marginLeft="50dp"
				android:layout_toRightOf="@+id/guid"
				android:layout_height="wrap_content" 
				android:textSize="13dp"  />		
				
			<TextView android:id="@+id/synced" android:text="synced"
				android:layout_toRightOf="@+id/id"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 
				android:layout_marginLeft="50dp"
				android:textSize="13dp" />	
				
			<TextView android:id="@+id/in_prompt" android:text="In:" 
				android:textSize="13dp"
				android:layout_below="@+id/guid" android:layout_width="wrap_content" android:layout_height="wrap_content"/>			
				
			<TextView android:id="@+id/syringes_in" 
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:singleLine="true"
				android:textSize="13dp" android:layout_marginLeft="5dp"
				android:layout_below="@+id/guid"
				android:layout_toRightOf="@+id/in_prompt" />

			<TextView android:id="@+id/out_prompt" android:text="Out:" 
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 
				android:textSize="13dp"
				android:layout_marginLeft="5dp"
				android:layout_below="@+id/guid"				
				android:layout_toRightOf="@+id/syringes_in"	/>

			<TextView android:id="@+id/syringes_out"
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:singleLine="true"
				android:textSize="13dp" 
				android:layout_marginLeft="3dp"
				android:layout_below="@+id/guid"
				android:layout_toRightOf="@+id/out_prompt" />
				
			<TextView android:id="@+id/timestamp"
				android:text="time/date"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginLeft="3dp" android:layout_marginTop="3dp"
				android:layout_below="@+id/out_prompt" android:textSize="13dp" />

			<TextView android:id="@+id/latitude"
				android:text="lat"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginLeft="20dp" android:layout_marginTop="3dp"
				android:layout_below="@+id/out_prompt" 
				android:layout_toRightOf="@id/timestamp"
				android:textSize="13dp" />

			<TextView android:id="@+id/longitude"
				android:text="long"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_below="@+id/out_prompt" 				
				android:layout_toRightOf="@id/latitude"
				android:layout_marginTop="3dp" android:layout_marginLeft="3dp"
				android:textSize="13dp" />

<!-- 			<TextView android:id="@+id/idNumberText" -->
<!-- 				android:layout_width="wrap_content" android:layout_height="wrap_content" -->
<!-- 				android:layout_marginLeft="2dp" android:layout_marginRight="2dp" -->
<!-- 				android:layout_below="@id/status" android:textSize="13dp" -->
<!-- 				android:layout_alignParentRight="true" android:layout_alignTop="@id/longitude" /> -->
<!-- 		</RelativeLayout> -->
	</RelativeLayout>
		<requestFocus></requestFocus>
</LinearLayout>